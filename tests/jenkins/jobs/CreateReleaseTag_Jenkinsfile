lib = library(identifier: 'jenkins@1.0.0', retriever: modernSCM([
    $class: 'GitSCMSource',
    remote: 'https://github.com/opensearch-project/opensearch-build-libraries.git',
]))

pipeline {
    agent none
    stages {
        stage('createTag') {
            steps {
                script {
                    createReleaseTag(
                            distManifest: "tests/data/opensearch-build-1.1.0.yml",
                            tagVersion: "1.1.0"
                    )
                }
            }
        }
    }
}