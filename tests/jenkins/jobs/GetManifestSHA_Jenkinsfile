pipeline {
    agent none
    stages {
        stage('manifest SHA') {
            steps {
                script {
                    def sha = getManifestSHA(
                        manifest: 'tests/jenkins/data/opensearch-1.3.0.yml',
                        platform: 'linux',
                        architecture: 'x64'
                    )

                    echo "sha: ${sha.sha}"
                    echo "exists: ${sha.exists}"
                    echo "lock: ${sha.lock}"
                    echo "path: ${sha.path}"
                }
            }
        }
    }
}
