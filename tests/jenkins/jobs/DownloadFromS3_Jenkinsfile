pipeline {
    agent none
    stages {
        stage('download') {
            steps {
                script {
                    downloadFromS3(
                        destPath: '/tmp/src/path',
                        bucket: 'dummy_bucket',
                        path: '/download/path',
                        force: true
                    )
                }
            }
        }
    }
}