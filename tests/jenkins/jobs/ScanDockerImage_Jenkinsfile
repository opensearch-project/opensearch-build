pipeline {
    agent none
    stages {
        stage('scan docker image') {
            steps {
                script {
                    scanDockerImage(
                        imageFullName: "opensearchstaging/opensearch:2.0.0",
                        imageResultFile: "scan_docker_image"
                    )
                }
            }
        }
    }
}
