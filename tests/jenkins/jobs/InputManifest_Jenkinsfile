def lib = library("jenkins").jenkins

pipeline {
    agent none
    stages {
        stage('Input Manifest Properties') {
            steps {
                script {
                    def inputManifest = lib.InputManifest.new(this, readYaml(file: "manifests/1.2.0/opensearch-1.2.0.yml"))
                    echo inputManifest.ci.dockerImage
                    echo inputManifest.ci.dockerArgs
                }
            }
        }
    }
}