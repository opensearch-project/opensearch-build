pipeline {
    agent none
    stages {
        stage('upload manifest SHA') {
            steps {
                script {
                    copyDockerImage(
                        sourceImagePath: "opensearchstaging:latest",
                        destinationImagePath: "opensearchproject:latest",
                        destinationType: "ecr",
                        destinationCredentialIdentifier: "path_to_ecr/repo_id/"
                    )
                }
            }
        }
    }
}
