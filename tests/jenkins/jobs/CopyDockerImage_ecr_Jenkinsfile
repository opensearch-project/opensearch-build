pipeline {
    agent none
    stages {
        stage('upload manifest SHA') {
            steps {
                script {
                     copyDockerImage(
                        sourceImagePath: sourceImagePath,
                        destinationImagePath: destinationImagePath,
                        destinationType: destinationType,
                        destinationCredentialIdentifier: destinationCredentialIdentifier,
                        accountName: accountName
                    )
                }
            }
        }
    }
}
