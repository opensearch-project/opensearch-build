lib = library(identifier: 'jenkins@1.0.0', retriever: modernSCM([
    $class: 'GitSCMSource',
    remote: 'https://github.com/opensearch-project/opensearch-build-libraries.git',
]))

pipeline {
    agent none
    stages {
        stage('sign') {
            steps {
                script {
                    uploadToS3(
                            sourcePath: '/tmp/src/path',
                            bucket: 'dummy_bucket',
                            path: '/upload/path'
                    )
                }
            }
        }
    }
}
